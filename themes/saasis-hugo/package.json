{
  "name": "saasis-hugo",
  "description": "saas theme for hugo",
  "version": "3.3.2",
  "license": "UNLICENSED",
  "author": "gethugothemes",
  "scripts": {
    "dev": "cd exampleSite && hugo server --themesDir ../..",
    "build": "cd exampleSite && hugo --gc --minify --templateMetrics --templateMetricsHints --forceSyncStatic --themesDir ../..",
    "vercel-build": "./vercel-build.sh",
    "preview": "cd exampleSite && hugo server --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --watch --forceSyncStatic -e production --minify --themesDir ../..",
    "update-modules": "node ./scripts/clearModules.js && hugo mod clean --all && hugo mod get -u ./... && hugo mod tidy"
  },
  "devDependencies": {
    "prettier": "^3.3.2",
    "prettier-plugin-go-template": "0.0.15"
  }
}
